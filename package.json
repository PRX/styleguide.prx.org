  "scripts": {
    "ng": "ng",
    "start": "npm run storybook",
    "demo": "npm run build:lib && ng serve ngx-prx-styleguide-demo",
    "build": "ng build --prod=true",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "npm run build ngx-prx-styleguide && npm run build:lib:assets",
    "build:lib:watch": "ng build ngx-prx-styleguide --prod=true --watch",
    "postbuild:lib:watch": "node ./bin/buildwatch.js",
    "build:lib:assets": "./bin/buildassets.sh",
    "version": "node ./bin/readver.js | ./bin/newver.sh",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "ci": "jest --ci --coverage"
  },
