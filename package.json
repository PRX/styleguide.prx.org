{
  "name": "ngx-prx-styleguide",
  "version": "2.3.0",
  "description": "PRX Angular 6 style guide library",
  "keywords": [
    "PRX",
    "Styleguide"
  ],
  "homepage": "https://github.com/prx/styleguide.prx.org",
  "bugs": {
    "url": "https://github.com/prx/styleguide.prx.org/issues",
    "email": "help@prx.org"
  },
  "author": "PRX, Inc. <help@prx.org> (http://www.prx.org)",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
