    "rxjs": "~6.0.0",
    "scss-bundle": "~2.4.0",
    "zone.js": "~0.8.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular-devkit/core": "~7.3.5",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@babel/core": "~7.3.4",
    "@storybook/addon-a11y": "^5.0.5",
    "@storybook/addon-actions": "~5.0.5",
    "@storybook/addon-backgrounds": "~5.0.5",
    "@storybook/addon-centered": "~5.0.5",
    "@storybook/addon-knobs": "~5.0.5",
    "@storybook/addon-links": "~5.0.5",
    "@storybook/addon-notes": "~5.0.5",
    "@storybook/addon-options": "~5.0.5",
    "@storybook/addon-storysource": "~5.0.5",
    "@storybook/addon-viewport": "^5.0.5",
    "@storybook/addons": "~5.0.5",
    "@storybook/angular": "~5.0.5",
    "@types/c3": "~0.4.41",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/pikaday": "0.0.32",
    "@types/storybook__addon-actions": "~3.4.2",
    "@types/storybook__addon-knobs": "~4.0.4",
    "babel-loader": "~8.0.5",
    "codelyzer": "~4.5.0",
    "css-loader": "~2.1.1",
    "dotenv": "~2.0.0",
    "i": "~0.3.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
